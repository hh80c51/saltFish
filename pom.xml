        <aspectjrt.version>1.8.9</aspectjrt.version>
        <jackson.version>1.9.13</jackson.version>
        <jackson.fasterxml.version>2.8.4</jackson.fasterxml.version>
        <javamail.version>1.4.7</javamail.version>
