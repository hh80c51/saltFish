    <!-- 属性声明 -->
    <properties>
        <spring-web.version>5.1.5.RELEASE</spring-web.version>
        <spring-orm.version>5.0.8.RELEASE</spring-orm.version>
        <spring-webmvc.version>5.1.14.RELEASE</spring-webmvc.version>
