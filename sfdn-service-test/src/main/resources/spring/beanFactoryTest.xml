<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="myTestBean" class="bean.springbean.MyTestBean">
        <meta key="testStr" value="aaaaaaaaaaaa"/>
    </bean>
    <!-- 1.使用bean标签的name属性增加别名 -->
    <bean id="myTestBean" class="bean.springbean.MyTestBean" name="testBean, testBean2"/>
    <!-- 2.使用<alias>增加别名 -->
    <bean id="myTestBean" class="bean.springbean.MyTestBean"/>
    <alias name="testBean" alias="testBean,testBean2"/>

    <import resource="beanFactoryTest.xml"/>
    <import resource="constructArgTest.xml"/>
    <beans>
        <bean id="myTestBean" class="bean.springbean.MyTestBean"/>
    </beans>
</beans>