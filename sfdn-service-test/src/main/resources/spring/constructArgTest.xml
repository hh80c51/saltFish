<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:mybean="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <beans>
        <bean id="user" class="bean.springbean.User">
            <constructor-arg index="0">
                <value>郝佳</value>
            </constructor-arg>
            <constructor-arg index="1">
                <value>你好</value>
            </constructor-arg>
        </bean>
        <bean id="test" class="bean.springbean.MyTestBean">
            <property name="testStr" value="aaa"/>
        </bean>

        <bean id="test1" class="bean.springbean.MyTestBean">
            <property name="arglist">
                <list>
                    <value>aa</value>
                    <value>bb</value>
                </list>
            </property>
        </bean>
        <bean id="myTestBean" class="bean.springbean.MyTestBean">
            <qualifier type="org.springframework.beans.factory.annotation.Qualifier" value="qf"/>
        </bean>
        <bean id="myTestBean" class="bean.springbean.MyTestBean">
            <mybean:user username="aaa"/>
        </bean>
    </beans>
</beans>